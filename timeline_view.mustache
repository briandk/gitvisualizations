<html>
<head>
  <title>Timeline</title>
  <meta charset="utf-8">
  <link rel="stylesheet" type="text/css" href="TimelineStyle.css">
  <link rel="stylesheet" type="text/css" href="prism.css">
  <link rel="stylesheet" type="text/css" href="bootstrap.css">
  <script>
    var revisions = [{{revisions}}];
  </script>
</head>
<body>
  <table>
    <tr>
      {{#snapshots}}
        <td>
          <div class="timestamp">
            <time id="{{revision}}">{{timestamp}}</time>
          </div>
          <div class="codesnapshot">
            <pre class="language-clike" data-line="{{changedLines}}"><code>{{code}}</code>
          </div>
          </pre>
        </td>
      {{/snapshots}}
    </tr>
  </table>
  <div class="navbar navbar-fixed-bottom">
    <div class="navbar-inner">
      <a class="brand" href="#">CodeTimeline</a>
      <ul class="nav">
        <div class="btn-toolbar">
          <div class="btn-group">
            <button type="button" class="btn btn-info" data-toggle="button">X-ray</button>
          </div>
          <div class="btn-group">
            <button type="button" class="btn">&lt;</button>
            <button type="button" class="btn">&gt;</button>
          </div>
        </div>
      </ul>
    </div>
  </div>
  <script src="prism.js" type="text/javascript"></script>
  <script src="jQuery.js" type="text/javascript"></script>
  <script src="bootstrap.min.js" type="text/javascript"></script>
</body>
</html>
